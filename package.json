{
    "name": "sumber-bening-app",
    "version": "0.1.0",
    "author": "Raihan Firmansyah",
    "private": true,
    "homepage": ".",
    "description": "Sumber Bening web and desktop App",
    "main": "public/electron.js",
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.10",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^12.8.3",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "electron-is-dev": "^2.0.0",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "react-to-print": "^2.13.0",
        "swiper": "^6.7.5",
        "web-vitals": "^1.1.1"
    },
    "scripts": {
        "start": "cross-env REACT_APP_ROUTER=Browser react-scripts start",
        "start:hash": "set PORT=3131 && react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"cross-env BROWSER=none npm run start:hash\" \"wait-on http://localhost:3131 && electron .\"",
        "electron-pack": "npx electron-builder",
        "preelectron-pack": "npm run build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^6.2.1",
        "cross-env": "^7.0.3",
        "electron": "^16.0.5",
        "electron-builder": "^22.11.7",
        "react-snapshot": "^1.3.0",
        "wait-on": "^6.0.0"
    },
    "build": {
        "appId": "com.loyno-mathcs.RRRHAN.weatherstation.wotdevice",
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "productName": "Sumber Bening App",
        "target": "NSIS",
        "nsis": {
            "allowToChangeInstallationDirectory": true,
            "oneClick": false
        },
        "directories": {
            "buildResources": "assets"
        },
        "win": {
            "icon": "public/favicon.ico"
        }
    }
}